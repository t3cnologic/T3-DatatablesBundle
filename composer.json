{
    "name": "t3/datatablesbundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Datatable Bundle For Doctrine2 Entities",
    "keywords": ["symfony2", "datatable", "grid", "table", "datagrid", "pagination"],
    "homepage": "https://github.com/t3cnologic/T3-DatatablesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "T3cnologic"
        },
        {
            "name": "T3DatatablesBundle contributors",
            "homepage": "https://github.com/t3cnologic/T3-DatatablesBundle/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/t3cnologic/T3-DatatablesBundle/issues"
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3",
        "friendsofsymfony/jsrouting-bundle": "@stable"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3"
    },
    "suggest": {
        "twbs/bootstrap": "v3.2.0",
        "mopa/bootstrap-bundle": "v3.0.0-beta4",
        "components/jquery": "1.11.1",
        "datatables/datatables": "1.10.4",
        "moment/moment": "2.8.4"
    },
    "autoload": {
        "psr-0": { "T3\\DatatablesBundle": "" }
    },
    "target-dir": "T3/DatatablesBundle"
}
